<h1>TOP PAGE</h1>
<%= link_to "新規投稿", "/posts/new" %>
<%@posts.each do |post|%>
<div class="post">
    <div class="post-date">
      投稿日時：<%= post.created_at %>
    </div>
    <div class="post-content">
      <%= post.content %>
    </div>
    <%= link_to  "編集", "/posts/#{post.id}/edit", method: :get %>
    <%= link_to  "削除", "/posts/#{post.id}", method: :delete %>
  </div>
<% end %>